{
  "name": "cf-ai-searcheragent",
  "type": "module",
  "scripts": {
    "dev:tailwind": "bunx @tailwindcss/cli -i ./src/public/input.css -o ./src/public/output.css --watch",
    "dev:wrangler": "wrangler dev",
    "dev": "concurrently \"bunx @tailwindcss/cli -i src/public/input.css -o src/public/output.css --watch\" \"wrangler dev\"",
    "dev:remote": "wrangler dev --remote",
    "db:generate": "drizzle-kit generate",
    "db:up": "drizzle-kit up",
    "deploy": "wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.4",
    "@tailwindcss/cli": "^4.1.17",
    "agents": "^0.2.13",
    "drizzle-orm": "^0.44.6",
    "hono": "^4.9.10",
    "openai": "^6.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cloudflare/puppeteer": "^1.0.4",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.5",
    "prettier-plugin-classnames": "^0.8.5",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.17",
    "wrangler": "^4.43.0"
  }
}
